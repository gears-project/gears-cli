use shell::{Command};

pub expression -> Command
  = set
  / help
  / sync
  / other

set -> Command
  = ".set" _ key:label _ val:label {
    Command::Set(key, val)
  }

help -> Command
 = ".help" {
   Command::Help
 }

sync -> Command
 = ".sync" {
   Command::Sync
 }

other -> Command
  = s:$([A-Za-z0-9\_\ ]+) {
    Command::Other(s.to_string())
  }

label -> String
  = label:$([A-Za-z0-9\_]+) {
    label.to_string()
  }

_ -> ()
  = space*

space -> ()
  = " "

