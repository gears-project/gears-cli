use shell::{Command};

pub expression -> Command
  = set
  / help
  / sync

set -> Command
  = ".set" _ key:label _ val:label {
    Command::Set(key, val)
  }

help -> Command
 = ".help" { Command::Help }

sync -> Command
 = ".sync" { Command::Sync }

label -> String
  = label:$([A-Za-z0-9\_]+) {
    label.to_string()
  }

_ -> ()
  = space*

space -> ()
  = " "

